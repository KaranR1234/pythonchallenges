def make_it_laugh(input_string):
    vowels = "AEIOUaeiou"
    modified_string = ""
    
    for char in input_string:
        if char in vowels:
            modified_string += "haha"
        else:
            modified_string += char
    
    print(modified_string)

input_string = "Hello, how are you today?"
make_it_laugh(input_string)
